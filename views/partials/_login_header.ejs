<head>
  <link rel="stylesheet" href="styles/newsarticles.css">
  <script src="https://kit.fontawesome.com/9566ef13c4.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
  <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
  <script src="logout.js" async></script>
  
</head>
<header>
  <nav class="nav div-strip">
      <% if (!uId) { %>
        <a class="nav-link" href="/login">Log In</a>
        
      <% } %>
      
      <a class="nav-link" href="/register">Sign Up</a>
      <% if (uId) { %>
        <a class="nav-link mobile" href="/user/user/profile">Settings</a>
				<a class="nav-link mobile" href="/user/user/feed">My Feed</a>
				<a class="nav-link mobile" href="/user/user/bookmarks">Bookmarks</a>
        <a class="nav-link mobile header-top-nav-logout-link" id="header-top-nav-logout-link"> Log Out </a>
        <div class="dropdown"><i class="fas fa-user dropbtn"></i> <%= uId %>
          <div class="dropdown-content">
            <a href="/user/<%= uId %>/profile">Settings</a>
						<a href="/user/<%= uId %>/feed">My Feed</a>
						<a href="/user/<%= uId %>/bookmarks">Bookmarks</a>
            <a class="nav-link desktop header-top-nav-logout-link header-top-nav-logout-link-menu-click" id="header-top-nav-logout-link-menu-click"> Log Out </a>
          </div>
        </div>
      <% } %>
  </nav>
</header>